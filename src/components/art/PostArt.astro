---
import Warhol1 from "./Warhol1.astro";
import Noir1 from "./Noir1.astro";
import Splash1 from "./Splash1.astro";
import Grid1 from "./Grid1.astro";

interface Props {
  art: "warhol-1" | "noir-1" | "splash-1" | "grid-1";
  compact?: boolean;
}

const { art, compact = false } = Astro.props;
---

{art === "warhol-1" && <Warhol1 compact={compact} />}
{art === "noir-1" && <Noir1 compact={compact} />}
{art === "splash-1" && <Splash1 compact={compact} />}
{art === "grid-1" && <Grid1 compact={compact} />}
